{
    "manifest_version": 2,

    "name": "URL-Shortener",
    "description": "This extension will help you to Shortener URL",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "URL-Shortener",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png",
        "256": "icon.png"
    },
    "permissions": [
        "tabs"
    ]
}