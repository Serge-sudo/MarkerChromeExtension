{
    "manifest_version": 2,

    "name": "Fast Search",
    "description": "This extension will help you search faster",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "The Title",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png",
        "256": "icon.png"
    },
    "permissions": [
        "tabs",
        "tabs",
        "clipboardWrite",
        "http://*/*",
        "https://*/*"

    ]
}